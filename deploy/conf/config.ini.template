[default]
DEBUG=True

[database]
DB_NAME=${DB_NAME}
DB_USER=${DB_USER}
DB_HOST=${DB_HOST}
DB_PORT=${DB_PORT}
DB_PASSWORD=${DB_PASSWORD}

[secrets]
SECRET_KEY=${DJANGO_SECRET_KEY}
DISCOURSE_SSO_SECRET=${DISCOURSE_SSO_SECRET:-changeme}

[logging]
LOGGING_DIRECTORY=/code/logs

[storage]
DATA_STORAGE_ROOT=/data
LIBRARY_ROOT=/library
REPOSITORY_ROOT=/repository

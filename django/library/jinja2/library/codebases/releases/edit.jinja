{% extends "base.jinja" %}

{% block introduction %}
    <h1><i class='fas fa-cloud-upload-alt'></i> Archive your computational model</h1>
{% endblock %}
{% block content %}
    <div id="app" data-version-number="{{ release.version_number }}" data-identifier="{{ release.codebase.identifier }}"
         data-review-status-enum="{{ to_json(review_status_enum) }}"></div>
{% endblock %}

{% block js %}
{{ render_bundle('releases', attrs='defer') }}
{% endblock %}
